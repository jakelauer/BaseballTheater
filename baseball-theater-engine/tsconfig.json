{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "outDir": "dist",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "declaration": true,
    "skipLibCheck": true,
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "noEmit": false,
    "paths": {
      "@Utility": [
        "engine/utils"
      ]
    }
  }
}
