{
  "name": "baseball-theater-v2",
  "version": "1.0.0",
  "description": "",
  "main": "server/server.ts",
  "scripts": {
    "installall": "yarn install --check-files && cd client && yarn install --check-files",
    "client": "cd client && yarn start",
    "server": "node -r ts-node/register --inspect=5001 --nolazy server/server.ts --runInBand",
    "dev": "concurrently --kill-others \"yarn server\" \"yarn client\"",
    "buildserver": "node server/scripts/build.js",
    "build": "(if exist output rmdir /s /q output) && cd client && yarn build && cd ../ && yarn buildserver",
    "finalize": "node server/scripts/finalize.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "abortcontroller-polyfill": "^1.3.0",
    "cookie-parser": "^1.4.4",
    "cross-fetch": "^3.0.4",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "express-serve-static-core": "^0.1.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "mongodb": "^3.3.2",
    "simple-oauth2": "^3.1.0",
    "client-oauth2": "^4.2.5",
    "ts-node": "^8.3.0",
    "typescript": "3.7.2",
    "uuid-apikey": "^1.4.4",
    "x2js": "^3.3.1"
  },
  "devDependencies": {
    "@types/compression": "^1.0.1",
    "@types/cookie-parser": "^1.4.2",
