    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-react": "^3.23.0",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "concurrently": "^4.0.1",
    "eslint": "^6.3.0",
    "eslint-plugin-react": "^7.14.3",
    "fs-extra": "^8.1.0",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-react": "^3.23.11",
    "ts-loader": "^6.1.0",
    "webpack": "4.39.1",
    "webpack-cli": "^3.3.8"
  }
